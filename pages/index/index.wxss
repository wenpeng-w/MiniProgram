/**index.wxss**/
.startup-page {
  padding: 300rpx 0 100rpx 0;
}
.origin {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.origin-pic {
  width: 400rpx;
  height: 80rpx;
}
.user-info {
  text-align: center;
}
.user-avatar {
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto 50rpx auto;
  border-radius: 100%;
  overflow: hidden;
}
.user-address {
  color: #333;
  font-weight: bold;
}